{
	"chrome_url_overrides": {
		"newtab": "newtab.html"
	},
	"description": "A speed dial functionally similar to the one used in Opera.",
	"icons": {
		"128": "icons/icon128.png",
		"48": "icons/icon48.png",
		"16": "icons/icon16.png"
	},
	"manifest_version": 2,
	"name": "Minimal Speed Dial",
	"options_page": "options.html",
	"permissions": [ "background", "bookmarks", "storage", "<all_urls>" ],
	"background": { "scripts": [ "javascript/eventPage.js" ], "persistent": false },
	"version": "1.0.0"
}
